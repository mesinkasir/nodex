<%- include("partials/header"); -%>
<%- include("partials/navbar"); -%>
    <h1>Blog Content</h1>
    <form class="" action="/nodex" method="post">
      <div class="form-group">
        <label>Title</label>
        <input class="form-control" type="text" name="postTitle">
        <label>Image URL</label>
        <input class="form-control" type="text" name="postImg">
        <label>Post</label>
        <textarea class="form-control" name="postBody" rows="5" cols="30"></textarea>
      </div><br/>
      <button class="btn btn-info rounded-pill col-4 float-end p-1" type="submit" name="button">Publish</button>
    </form>
<%- include("partials/footer"); -%>
